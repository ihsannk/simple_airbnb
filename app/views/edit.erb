<div class="container">

  <form action="/property/<%= @property.id%>/edit" method="post">
    <p>Property name: <input type="text" name="input_property_name" value="<%= @property.property_name %>"><p>
    <p>Description: <input type="text" name="input_description" value="<%= @property.description %>"></p>
    <p>Price: <input type="text" name="input_price" value="<%= @property.price%>"></p>
    <p>Tags: <input type="text" name="input_tag" value="<%= @tag_string %>">

    <p><input type="submit" value="Edit"></p>
  </form>


</div>